.downNorm {
  padding: 34rpx 56rpx 0rpx 56rpx;
}

.downNorm .downNorm-header {
  margin-bottom: 36rpx;
  display: flex;
  align-items: flex-start;
}

.downNorm .downNorm-header .downNorm-header-success-img {
  flex-shrink: 0;
  width: 40rpx;
  height: 40rpx;
  margin-right: 18rpx;
}

.downNorm .downNorm-header .downNorm-header-txt {
  flex: 1;
  overflow-x: hidden;
  font-size: 30rpx;
  color: #999999;
  line-height: 44rpx;
}

.downNorm .save-box {
  margin-bottom: 48rpx;
  width: 100%;
}

.downNorm .save-box .save-img {
  width: 100%;
}

.downNorm .down-btn-con {
  margin-bottom: 98rpx;
  display: flex;
  justify-content: center;
}

.downNorm .down-btn-con .down-btn {
  cursor: pointer;
  flex-shrink: 0;
  font-size: 28rpx;
  color: #669cf6;
  text-decoration: underline;
}

.downNorm .sub_wrap {
  padding: 0 34rpx;
}

.downNorm .sub_wrap button {
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  padding: 0;
  box-shadow: 0 4rpx 8rpx 0 rgba(56, 116, 246, 0.1);
  border-radius: 16rpx;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  background: #669cf6;
  color: #ffffff;
  text-align: center;
  margin-bottom: 40rpx;
}

.downNorm .sub_wrap ._but {
  background: #f2f2f2;
  color: #666666;
  margin: 0;
}
