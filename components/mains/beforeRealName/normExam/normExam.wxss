._questionnaire {
  padding: 50rpx 40rpx 20rpx;
  background: #ffffff;
}

._questionnaire ._header {
  background: #ffffff;
  margin-bottom: 64rpx;
}

._questionnaire ._header ._p1 {
  text-align: center;
  font-family: PingFangSC-Medium;
  font-size: 44rpx;
  color: #669cf6;
  line-height: 60rpx;
  height: 60rpx;
  margin-bottom: 64rpx;
}

._questionnaire .des-for-start {
  margin-bottom: 40rpx;
}

._questionnaire ._subject ._singleSelection {
  margin-bottom: 80rpx;
}

._questionnaire ._subject ._singleSelection ._title {
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  color: #111500;
  line-height: 44rpx;
  display: flex;
  margin-bottom: 24rpx;
}

._questionnaire ._subject ._singleSelection ._must::before {
  content: "*  ";
  color: red !important;
  width: 20rpx;
  font-size: 32rpx;
  height: 44rpx;
  line-height: 48rpx;
  display: inline-block;
  text-align: left;
  overflow: hidden;
}

._questionnaire ._subject ._singleSelection ._options {
  width: 100%;
  background: #ffffff;
  border: 2rpx solid #eeeeee;
  border-radius: 12rpx;
}

._questionnaire ._subject ._singleSelection ._options .radio_label {
  display: flex;
  border-bottom: 2rpx solid #eeeeee;
  padding: 10rpx 0 10rpx 24rpx;
  align-items: flex-start;
}

._questionnaire ._subject ._singleSelection ._options .radio_label:last-child {
  border-bottom: none;
}

._questionnaire ._subject ._singleSelection ._options .radio_label radio {
  margin: auto 0;
}

._questionnaire ._subject ._singleSelection ._options .radio_label ._text {
  min-height: 96rpx;
  line-height: 50rpx;
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #233347;
  display: flex;
  align-items: center;
}

._questionnaire ._subject ._singleSelection ._options .radio_label input {
  height: 64rpx;
  margin: auto 10rpx auto 24rpx;
  padding: 0 10rpx;
  border: 2rpx solid #eeeeee;
}

._questionnaire ._subject ._multipleChoice {
  margin-bottom: 80rpx;
}

._questionnaire ._subject ._multipleChoice ._title {
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  color: #111500;
  line-height: 44rpx;
  display: flex;
  margin-bottom: 24rpx;
}

._questionnaire ._subject ._multipleChoice ._must::before {
  content: "*  ";
  color: red !important;
  width: 20rpx;
  font-size: 32rpx;
  height: 44rpx;
  line-height: 48rpx;
  display: inline-block;
  text-align: left;
  overflow: hidden;
}

._questionnaire ._subject ._multipleChoice ._options {
  width: 100%;
  background: #ffffff;
  border: 2rpx solid #eeeeee;
  border-radius: 12rpx;
}

._questionnaire ._subject ._multipleChoice ._options .checkbox_label {
  display: flex;
  border-bottom: 2rpx solid #eeeeee;
  padding: 0 0 0 24rpx;
}

._questionnaire ._subject ._multipleChoice ._options .checkbox_label:last-child {
  border-bottom: none;
}

._questionnaire ._subject ._multipleChoice ._options .checkbox_label checkbox {
  margin: auto 0;
}

._questionnaire ._subject ._multipleChoice ._options .checkbox_label ._text {
  height: 96rpx;
  line-height: 96rpx;
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #233347;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

._questionnaire ._subject ._multipleChoice ._options .checkbox_label input {
  height: 64rpx;
  margin: auto 10rpx auto 24rpx;
  padding: 0 10rpx;
  border: 2rpx solid #eeeeee;
}

._questionnaire ._subject ._gapFilling {
  margin-bottom: 80rpx;
}

._questionnaire ._subject ._gapFilling ._title {
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  color: #111500;
  line-height: 44rpx;
  display: flex;
  margin-bottom: 24rpx;
}

._questionnaire ._subject ._gapFilling ._must::before {
  content: "*  ";
  color: red !important;
  width: 20rpx;
  font-size: 32rpx;
  height: 44rpx;
  line-height: 48rpx;
  display: inline-block;
  text-align: left;
  overflow: hidden;
}

._questionnaire ._subject ._gapFilling input {
  padding: 0 20rpx;
  background: #ffffff;
  border: 2rpx solid #eeeeee;
  border-radius: 6px;
  height: 76rpx;
  line-height: 76rpx;
}

._questionnaire ._subject ._gapFilling .picker {
  height: 76rpx;
  line-height: 76rpx;
  display: flex;
}

._questionnaire ._subject ._gapFilling .picker ._date1 {
  flex: 1;
  margin-left: 24rpx;
  border: 2rpx solid #eeeeee;
  border-radius: 6px;
  height: 76rpx;
  line-height: 76rpx;
}

._questionnaire ._subject .sub_wrap button {
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  padding: 0;
  box-shadow: 0 4rpx 8rpx 0 rgba(56, 116, 246, 0.1);
  border-radius: 16rpx;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  background: #669cf6;
  color: #ffffff;
  text-align: center;
  margin-bottom: 40rpx;
}

._questionnaire ._subject .sub_wrap ._but {
  background: #f2f2f2;
  color: #666666;
  margin: 0;
}
