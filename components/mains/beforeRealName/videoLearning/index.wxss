.videoLearning {
  padding: 0rpx 80rpx;
}

.videoLearning .videoLearning-img {
  width: 202rpx;
  height: 202rpx;
  margin: 120rpx auto 0rpx;
  display: block;
}

.videoLearning .videoLearning-des {
  margin-top: 66rpx;
  width: 100%;
  text-align: center;
  font-size: 36rpx;
  color: #000000;
  line-height: 22px;
}

.videoLearning .videoLearning-des-mini {
  margin-bottom: 66rpx;
  box-sizing: border-box;
  font-size: 28rpx;
  color: #999999;
  line-height: 44rpx;
  margin-top: 34rpx;
}

.videoLearning .sub_wrap button {
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  padding: 0;
  box-shadow: 0 4rpx 8rpx 0 rgba(56, 116, 246, 0.1);
  border-radius: 16rpx;
  font-family: PingFangSC-Regular;
  font-size: 28rpx;
  background: #669cf6;
  color: #ffffff;
  text-align: center;
  margin-bottom: 40rpx;
}

.videoLearning .sub_wrap ._but {
  background: #f2f2f2;
  color: #666666;
  margin: 0;
}
