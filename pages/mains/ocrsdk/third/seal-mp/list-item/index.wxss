  color: #888888;
  margin-left: 8px;
  text-align: right;
}
.seal-list-item__body:empty,
.seal-list-item__value:empty {
  display: none;
}
.seal-list-item__body:empty + .seal-list-item__value {
  flex: 1;
  margin-left: 0;
}
.seal-list-item__right-icon {
  display: flex;
