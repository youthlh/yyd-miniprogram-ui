<!-- 活动   promotion -->
<wxs src="/utils/common.wxs" module="common" />
<view class="_activeData">
  <form catchsubmit="formSubmit" bindreset="formReset" wx:if="{{!hospitalIdShow}}" disabled="{{!readOnly}}">
    <view class="_activeDataCont">
      <!-- 1=未提交 10=已提交,待审核 11=审核不通过 20=审核通过,带验收 21=验收不通过 30=验收通过 -->
      <view class="_titText" wx:if="{{taskActivities.activityStatus === 11}}">
        {{taskActivities.approveReason?taskActivities.approveReason:'--'}}
      </view>
      <view style="padding:16rpx 20rpx 0" class="{{shadeShow?'_activeDataCont1':''}}">
        <view wx:if="{{common.indexOfData([1,20,21,22],taskActivities.taskType) }}">
          <view class="_header">
            <view class="_title">
              {{taskActivities.activityName?taskActivities.activityName:"--"}}
            </view>
            <view class="_corporationName">
              <view class="_name1">
                {{common.formatDate((taskActivities.activityDate + taskActivities.activityTime + 28800000),"date")}}
              </view>
              <view class="_name2">{{taskActivities.regionMc?taskActivities.regionMc:"--"}}</view>
            </view>
            <view class="_center">
              <view class="_centerle" bindtap="invitations" data-activityid="{{taskActivities.activityId}}" data-taskid="{{taskActivities.taskId}}" wx:if="{{readOnly}}">
                <view class="_icon iconfont icon-document"></view>
                邀请函
              </view>
              <view class="_centerle  {{readOnly?'_centerle1':''}}" bindtap="checkDetails" data-activityid="{{taskActivities.activityId}}" data-taskid="{{taskActivities.taskId}}">
                <view class="_icon iconfont icon-editor"></view>
                签到详情
              </view>
            </view>
          </view>
          <view class="_details">
            <view class="_title">基本信息</view>
            <view wx:if="{{common.indexOfData([1],taskActivities.taskType) }}">
              <view class="_specificationTitle">
                会议主题：
            {{taskActivities.conferenceThemeName?taskActivities.conferenceThemeName:"--"}}
              </view>
              <view class="_specificationTitle">
                产品名称：
                <view wx:for="{{taskActivities.taskProductSpecifications}}" wx:for-index="index" wx:key="index" data-index="{{index}}" class="_dataListChild">
                  {{item.specificationName?item.specificationName:"--"}}
                </view>
              </view>
              <view class="_specificationTitle">
                会议地点：
            {{taskActivities.activityLocation?taskActivities.activityLocation:"--"}}
              </view>
              <view class="_specificationTitle">
                医院名称：
            {{taskActivities.hospitalName?taskActivities.hospitalName:"--"}}
              </view>
              <view class="_specificationTitle">
                医院等级：
            {{taskActivities.gradeName?taskActivities.gradeName:"--"}}
              </view>
              <view class="_specificationTitle">
                科室：
            {{taskActivities.departmentName?taskActivities.departmentName:"--"}}
              </view>
            </view>
            <view class="_specificationTitle">
              备注：{{taskActivities.notes?taskActivities.notes:'--'}}
            </view>
          </view>
          <view class="_performance">
            <view class="_title">完成情况</view>
            <view class="_summary">会议总结：</view>
            <textarea :fixed="true" name="activitySummary" class="_address _marbot" disabled="{{!readOnly}}" auto-height bindinput="_bindinput" model:value="{{activitySummary}}" placeholder="请输入会议总结" placeholder-class="_hint" maxlength="5000">
              <text class="currentWordNumber" wx:if="{{readOnly}}">
                {{5000 -  activitySummary.length}}/5000
              </text>
            </textarea>
            <view class="_summary">上传会议方案照片：</view>
            <upload-img name="taskActivityPlanPhotos" imgList="{{taskActivityPlanPhotos}}" class="_marbot" readOnly="{{!readOnly}}" bind:getUploadImg="getUploadImg" num="{{_num}}" fileType="{{_fileType}}" urls="{{urls}}"></upload-img>
            <view class="_summary">上传会议现场照片：</view>
            <upload-img name="taskActivityScenePhotos" imgList="{{taskActivityScenePhotos}}" class="_marbot" readOnly="{{!readOnly}}" bind:getUploadImg="getUploadImg1" num="{{_num}}" fileType="{{_fileType}}" urls="{{urls}}"></upload-img>
          </view>
        </view>
        <view wx:if="{{common.indexOfData([23],taskActivities.taskType) }}">
          <view class="_header">
            <view class="_title">
              {{taskActivities.activityName?taskActivities.activityName:"--"}}
            </view>
            <view class="_corporationName">
              <view class="_name1">
                {{common.formatDate((taskActivities.activityDate + taskActivities.activityTime + 28800000),"date")}}
