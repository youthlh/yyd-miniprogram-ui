          </view>
          <view class="_performance">
            <view class="_title">完成情况</view>
            <view class="_summary">会议总结：</view>
            <textarea :fixed="true" name="activitySummary" class="_address _marbot" disabled="{{!readOnly}}" auto-height bindinput="_bindinput" model:value="{{activitySummary}}" placeholder="请输入会议总结" placeholder-class="_hint" maxlength="5000">
              <text class="currentWordNumber" wx:if="{{readOnly}}">
                {{5000 -  activitySummary.length}}/5000
              </text>
            </textarea>
            <view class="_summary">上传方案照片：</view>
            <upload-img name="taskActivityPlanPhotos" imgList="{{taskActivityPlanPhotos}}" class="_marbot" readOnly="{{!readOnly}}" bind:getUploadImg="getUploadImg" num="{{_num}}" fileType="{{_fileType}}" urls="{{urls}}"></upload-img>
            <view class="_summary">上传会议截图：</view>
            <upload-img name="taskActivityScenePhotos" imgList="{{taskActivityScenePhotos}}" class="_marbot" readOnly="{{!readOnly}}" bind:getUploadImg="getUploadImg1" num="{{_num}}" fileType="{{_fileType}}" urls="{{urls}}"></upload-img>
          </view>
        </view>
        <view wx:if="{{common.indexOfData([2,3,4,5,17,24,25],taskActivities.taskType) }}">
          <view class="_header">
            <view class="_title">
              {{taskActivities.activityName?taskActivities.activityName:"--"}}
            </view>
            <view class="_corporationName" style="height:1rpx;padding-bottom:12rpx;margin:0"></view>
          </view>
          <view class="_details">
            <view class="_title">基本信息</view>
            <view class="_specificationTitle">
              产品名称：
              <view wx:for="{{taskActivities.taskProductSpecifications}}" wx:for-index="index" wx:key="index" data-index="{{index}}" class="_dataListChild">
                {{item.specificationName?item.specificationName:"--"}}
              </view>
            </view>
            <view class="_specificationTitle">
              任务地区：
              {{taskActivities.regionMc?taskActivities.regionMc:"--"}}
            </view>
          </view>
          <view class="_performance">
            <view class="_title">完成情况</view>
            <view class="_questionnairetask" wx:if="{{taskActivities.questionnaireId}}">
              <view class="__questionnaireLet">
                <view class="_questionnaireTitle">{{taskActivities.questionnaireName}}</view>
                <view class="_p" bindtap="hasaData" data-activityid="{{taskActivities.activityId}}" data-taskid="{{taskActivities.taskId}}">
                  <view class="_p1">已答</view>
                  {{taskActivities.answeredCount}}
                </view>
              </view>
              <view class="__questionnairerig">
                <button class="_details" bindtap="minute" data-questionnaireid="{{taskActivities.questionnaireId}}">
                  分享
                </button>
              </view>
              <view style="clear: both"></view>
            </view>
          </view>
        </view>
        <view wx:if="{{common.indexOfData([6,7],taskActivities.taskType) }}">
          <view class="_header">
            <view class="_title">
              {{taskActivities.activityName?taskActivities.activityName:"--"}}
            </view>
            <view class="_corporationName" style="height:1rpx;padding-bottom:12rpx;margin:0"></view>
            <view class="_center" wx:if="{{!taskActivitySignIns.length}}">
              <view class="_centerle" bindtap="personalSign" data-activityid="{{taskActivities.activityId}}" data-taskid="{{taskActivities.taskId}}">
                <view class="_icon iconfont icon-document"></view>
                签到打卡
              </view>
            </view>
          </view>
          <view class="_details">
            <view class="_title">基本信息</view>
            <view class="_specificationTitle" wx:if="{{taskActivitySignIns.length}}">
              签到姓名：{{taskActivitySignIns[0]["personalName"]?taskActivitySignIns[0]["personalName"]:"--"}}
            </view>
            <view class="_specificationTitle" wx:if="{{taskActivitySignIns.length}}">
              签到地址：{{taskActivitySignIns[0]["place"]?taskActivitySignIns[0]["place"]:"--"}}
            </view>
            <view class="_specificationTitle" wx:if="{{taskActivitySignIns.length}}">
              签到时间：{{common.formatDate(taskActivitySignIns[0].createTime,"time")}}
            </view>
            <view class="_specificationTitle">
              产品名称：
              <view wx:for="{{taskActivities.taskProductSpecifications}}" wx:for-index="index" wx:key="index" data-index="{{index}}" class="_dataListChild">
                {{item.specificationName?item.specificationName:"--"}}
                {{taskActivities.taskProductSpecifications.length===1?'':index < taskActivities.taskProductSpecifications.length-1?',':''}}
              </view>
            </view>
            <view class="_specificationTitle">
              任务地区：{{taskActivities.regionMc?taskActivities.regionMc:"--"}}
            </view>
            <view class="_specificationTitle">
              地点：
            {{taskActivities.activityLocation?taskActivities.activityLocation:"--"}}
            </view>
