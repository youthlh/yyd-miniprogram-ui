              医院名称：
            {{taskActivities.hospitalName?taskActivities.hospitalName:"--"}}
            </view>
            <view class="_specificationTitle">
              医院等级：
            {{taskActivities.gradeName?taskActivities.gradeName:"--"}}
            </view>
            <view class="_specificationTitle">
              科室：
            {{taskActivities.departmentName?taskActivities.departmentName:"--"}}
            </view>
            <view class="_specificationTitle">
              手术类别：
            {{taskActivities.operationClassification?taskActivities.operationClassification:"--"}}
            </view>
            <view class="_specificationTitle">
              服务类别：
            {{taskActivities.serviceClassificationName?taskActivities.serviceClassificationName:"--"}}
            </view>
            <view class="_specificationTitle">
              备注：{{taskActivities.notes?taskActivities.notes:'--'}}
            </view>
          </view>
          <view class="_performance">
            <view class="_title">完成情况</view>
            <view wx:if="{{taskActivities.taskType===6}}">
              <view class="_summary">服务型跟台总结：</view>
              <textarea :fixed="true" name="activitySummary" class="_address _marbot" disabled="{{!readOnly}}" auto-height bindinput="_bindinput" model:value="{{activitySummary}}" placeholder="请输入服务型跟台总结" placeholder-class="_hint" maxlength="5000">
                <text class="currentWordNumber" wx:if="{{readOnly}}">
                  {{5000 -  activitySummary.length}}/5000
                </text>
              </textarea>
            </view>
            <view wx:elif="{{taskActivities.taskType===7}}">
              <view class="_summary">技术型跟台总结：</view>
              <textarea :fixed="true" name="activitySummary" class="_address _marbot" disabled="{{!readOnly}}" auto-height bindinput="_bindinput" model:value="{{activitySummary}}" placeholder="请输入技术型跟台总结" placeholder-class="_hint" maxlength="5000">
                <text class="currentWordNumber" wx:if="{{readOnly}}">
                  {{5000 -  activitySummary.length}}/5000
                </text>
              </textarea>
            </view>
          </view>
        </view>
        <view wx:if="{{common.indexOfData([8,9,10],taskActivities.taskType) }}">
          <view class="_header">
            <view class="_title">
              {{taskActivities.activityName?taskActivities.activityName:"--"}}
            </view>
            <view class="_corporationName" style="height:1rpx;padding-bottom:12rpx;margin:0"></view>
          </view>
          <view class="_details">
            <view class="_title">基本信息</view>
            <view class="_specificationTitle">
