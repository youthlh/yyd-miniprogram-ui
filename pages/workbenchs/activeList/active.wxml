              <view wx:for="{{taskActivities.taskProductSpecifications}}" wx:for-index="index" wx:key="index" data-index="{{index}}" class="_dataListChild">
                {{item.specificationName?item.specificationName:"--"}}
              </view>
            </view>
            <view class="_specificationTitle">
              任务地区：
              {{taskActivities.regionMc?taskActivities.regionMc:"--"}}
            </view>
          </view>
          <view class="_performance">
            <view class="_title">完成情况</view>
            <view wx:if="{{taskActivities.taskType===8}}">
              <view class="_summary">招标服务总结：</view>
              <textarea :fixed="true" name="activitySummary" class="_address _marbot" disabled="{{!readOnly}}" auto-height bindinput="_bindinput" model:value="{{activitySummary}}" placeholder="请输入招标服务总结" placeholder-class="_hint" maxlength="5000">
                <text class="currentWordNumber" wx:if="{{readOnly}}">
                  {{5000 -  activitySummary.length}}/5000
                </text>
              </textarea>
            </view>
            <view wx:elif="{{taskActivities.taskType===9}}">
              <view class="_summary">二次议价总结：</view>
              <textarea :fixed="true" name="activitySummary" class="_address _marbot" disabled="{{!readOnly}}" auto-height bindinput="_bindinput" model:value="{{activitySummary}}" placeholder="请输入二次议价总结" placeholder-class="_hint" maxlength="5000">
                <text class="currentWordNumber" wx:if="{{readOnly}}">
                  {{5000 -  activitySummary.length}}/5000
                </text>
              </textarea>
            </view>
            <view wx:elif="{{taskActivities.taskType===10}}">
              <view class="_summary">新品挂网总结：</view>
              <textarea :fixed="true" name="activitySummary" class="_address _marbot" disabled="{{!readOnly}}" auto-height bindinput="_bindinput" model:value="{{activitySummary}}" placeholder="请输入新品挂网总结" placeholder-class="_hint" maxlength="5000">
                <text class="currentWordNumber" wx:if="{{readOnly}}">
                  {{5000 -  activitySummary.length}}/5000
                </text>
              </textarea>
            </view>
            <view class="_summary topMargin" wx:if="{{taskActivities.taskType===8}}">
              上传中标通知照片：
            </view>
            <view class="_summary  topMargin" wx:elif="{{taskActivities.taskType===9}}">
              上传议价通知书：
            </view>
            <view class="_summary  topMargin" wx:elif="{{taskActivities.taskType===10}}">
              上传挂网通知书：
            </view>
            <upload-img name="taskActivityPhotos" imgList="{{taskActivityPhotos}}" class="_marbot" readOnly="{{!readOnly}}" bind:getUploadImg="getUploadtaskActivityPhotos" num="{{_num}}" fileType="{{_fileType}}" urls="{{urls}}"></upload-img>
          </view>
        </view>
        <view wx:if="{{common.indexOfData([16],taskActivities.taskType) }}">
          <view class="_header">
            <view class="_title">
              {{taskActivities.activityName?taskActivities.activityName:"--"}}
            </view>
            <view class="_corporationName" style="height:1rpx;padding-bottom:12rpx;margin:0"></view>
          </view>
          <view class="_performance">
            <view class="_title">完成情况</view>
            <view class="_summary">商业流向数据收集总结：</view>
            <textarea :fixed="true" name="activitySummary" class="_address _marbot" disabled="{{!readOnly}}" auto-height bindinput="_bindinput" model:value="{{activitySummary}}" placeholder="请输入商业流向数据收集总结" placeholder-class="_hint" maxlength="5000">
              <text class="currentWordNumber" wx:if="{{readOnly}}">
                {{5000 -  activitySummary.length}}/5000
              </text>
            </textarea>
            <view class="_summary">上传商业流向数据照片：</view>
            <upload-img name="taskActivityPhotos" imgList="{{taskActivityPhotos}}" class="_marbot" readOnly="{{!readOnly}}" bind:getUploadImg="getUploadtaskActivityPhotos" num="{{_num}}" fileType="{{_fileType}}" urls="{{urls}}"></upload-img>
          </view>
        </view>
        <view wx:if="{{common.indexOfData([11,12,13],taskActivities.taskType) }}">
          <view class="_header">
            <view class="_title">
              {{taskActivities.activityName?taskActivities.activityName:"--"}}
            </view>
            <view class="_corporationName" style="height:1rpx;padding-bottom:12rpx;margin:0"></view>
          </view>
          <view class="_details">
            <view class="_title">基本信息</view>
            <view class="_specificationTitle">
              产品名称：
              <view wx:for="{{taskActivities.taskProductSpecifications}}" wx:for-index="index" wx:key="index" data-index="{{index}}" class="_dataListChild">
                {{item.specificationName?item.specificationName:"--"}}
              </view>
            </view>
            <view class="_specificationTitle">
              任务地区：
              {{taskActivities.regionMc?taskActivities.regionMc:"--"}}
            </view>
          </view>
          <view class="_performance _activeFrom">
            <view class="form_item">
              <text class="title  _required">医院类别</text>
              <picker name="hospitalClassification" value="{{hospitalClassificationIndex}}" range-key="{{'label'}}" range="{{hospitalClassifications}}" bindchange="bindPickerChange" data-type="hospitalClassificationIndex">
                <view class="_picker">
                  {{hospitalClassifications[hospitalClassificationIndex].label}}
                </view>
              </picker>
              <em class="iconfont icon-right"></em>
            </view>
            <view class="form_item">
              <text class="title  _required">医院性质</text>
              <picker name="hospitalNature" value="{{hospitalNatureIndex}}" range-key="{{'label'}}" range="{{hospitalNatures}}" bindchange="bindPickerChange" data-type="hospitalNatureIndex">
                <view class="_picker">{{hospitalNatures[hospitalNatureIndex].label}}</view>
              </picker>
              <em class="iconfont icon-right"></em>
            </view>
            <view class="form_item">
              <text class="title  _required">医院名称</text>
              <input class="ipt" name="hospitalId" disabled bindinput="_bindinput" bindtap="_bindhospitalId" model:value="{{hospitalIdsSelect.hospitalName}}" data-type="activityName" placeholder="请选择医院名称" placeholder-class="_hint"></input>
              <em class="iconfont icon-right"></em>
            </view>
            <view class="form_item">
              <text class="title  _required">科室</text>
              <input class="ipt" name="departmentName" bindinput="_bindinput" model:value="{{departmentName}}" data-type="departmentName" placeholder="请输入科室名称" placeholder-class="_hint" maxlength="30"></input>
              <view class="iconfont  icon-close" bindtap="delIpt" wx:if="{{departmentName.length}}" data-type="departmentName"></view>
            </view>
            <view class="form_item">
              <text class="title  _required">门诊日流量(人次)</text>
              <input class="ipt" name="outpatientDailyCount" bindinput="_bindinput" model:value="{{outpatientDailyCount}}" data-type="outpatientDailyCount" placeholder="请输入门诊日流量(人次)" placeholder-class="_hint" maxlength="8" type="number"></input>
              <view class="iconfont  icon-close" bindtap="delIpt" wx:if="{{outpatientDailyCount.length}}" data-type="outpatientDailyCount"></view>
            </view>
            <view class="form_item">
              <text class="title  _required">医生数量(人)</text>
              <input class="ipt" name="doctorCount" bindinput="_bindinput" model:value="{{doctorCount}}" data-type="doctorCount" placeholder="请输入医生数量(人)" placeholder-class="_hint" maxlength="4" type="number"></input>
              <view class="iconfont  icon-close" bindtap="delIpt" wx:if="{{doctorCount.length}}" data-type="doctorCount"></view>
            </view>
            <view class="form_item">
              <text class="title  _required">床位数量(张)</text>
              <input class="ipt" name="bedCount" bindinput="_bindinput" model:value="{{bedCount}}" data-type="bedCount" placeholder="请输入床位数量(张)" placeholder-class="_hint" maxlength="4" type="number"></input>
              <view class="iconfont  icon-close" bindtap="delIpt" wx:if="{{bedCount.length}}" data-type="bedCount"></view>
            </view>
            <view class="form_item">
              <text class="title  _required">竞品名称</text>
              <input class="ipt" name="competitorName" bindinput="_bindinput" model:value="{{competitorName}}" data-type="competitorName" placeholder="请输入竞品名称" placeholder-class="_hint" maxlength="30"></input>
              <view class="iconfont  icon-close" bindtap="delIpt" wx:if="{{competitorName.length}}" data-type="competitorName"></view>
            </view>
            <view class="form_item">
              <text class="title  _required">竞品月销量</text>
              <input class="ipt" name="competitorMonthlySales" bindinput="_bindinput" model:value="{{competitorMonthlySales}}" data-type="competitorMonthlySales" placeholder="请输入竞品月销量" placeholder-class="_hint" maxlength="30" type="number"></input>
              <view class="iconfont  icon-close" bindtap="delIpt" wx:if="{{competitorMonthlySales.length}}" data-type="competitorMonthlySales"></view>
            </view>
            <view class="form_item">
              <text class="title  _required">药事会召开频率</text>
              <picker name="pharmaceuticalMeetingFrequency" value="{{pharmaceuticalMeetingFrequencyIndex}}" range-key="{{'label'}}" range="{{pharmaceuticalMeetingFrequencys}}" bindchange="bindPickerChange" data-type="pharmaceuticalMeetingFrequencyIndex">
                <view class="_picker">
                  {{pharmaceuticalMeetingFrequencys[pharmaceuticalMeetingFrequencyIndex].label}}
                </view>
              </picker>
              <em class="iconfont icon-right"></em>
            </view>
            <view class="form_item">
              <text class="title  _required _titleLong">有无慢病医保点或老年医学科</text>
              <picker name="existProblem" value="{{existProblemIndex}}" range-key="{{'label'}}" range="{{existProblems}}" bindchange="bindPickerChange" data-type="existProblemIndex">
                <view class="_picker">{{existProblems[existProblemIndex].label}}</view>
              </picker>
              <em class="iconfont icon-right"></em>
            </view>
            <view class="form_item  _note">
              <text class="title  _required">医院优势及潜力</text>
              <textarea :fixed="true" class="ipt _address" auto-height disabled="{{!readOnly}}" name="hospitalAdvantage" bindinput="_bindinput" model:value="{{hospitalAdvantage}}" data-type="hospitalAdvantage" placeholder="请输入医院优势及潜力" placeholder-class="_hint" maxlength="5000">
                <text class="currentWordNumber" wx:if="{{readOnly}}">
                  {{5000 -  hospitalAdvantage.length}}/5000
                </text>
              </textarea>
            </view>
          </view>
        </view>
        <view wx:if="{{common.indexOfData([14,15],taskActivities.taskType) }}">
          <view class="_header">
            <view class="_title">
              {{taskActivities.activityName?taskActivities.activityName:"--"}}
            </view>
